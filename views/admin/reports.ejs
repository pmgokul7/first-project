
<%-include("../partials/header.ejs")%>
<%-include("../partials/top.ejs")%>
<script src="https://code.jquery.com/jquery-3.5.1.js"></script>
<script src="https://cdn.datatables.net/1.13.1/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/buttons/2.3.2/js/dataTables.buttons.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
<script src="https://cdn.datatables.net/buttons/2.3.2/js/buttons.html5.min.js"></script>
<link rel="stylesheet" href="//cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css">
<div class="dropdown mt-4">
  <button class="btn btn-secondary dropdown-toggle mb-5" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
    filter
  </button>
  <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
    <li><a class="dropdown-item" href="/admin/reports">yearly Report</a></li>
    <li><a class="dropdown-item" href="/admin/report/month">Monthly Report</a></li>
    <li><a class="dropdown-item"  href="/admin/report/week">weekly Report</a></li>
    <!-- <li><a class="dropd mt-5 own-item" >weekly Report</a></li> -->
</div>
<h1><%=heading%></h1>
<table id="example" class="display nowrap " style="width:100%">
    <thead>
        <tr>
          <th>#</th>
          <th>UserName</th>
          <th>OrderId</th>			
          <th>Total Amount</th>			
          <th>Date of Order</th>
          <th>Status</th>
        </tr>
    </thead>
    <tbody>
      <% for(var i = 0 ;i<= result.length-1; i++) {%>             
        <tr>
            <td><%= i+1%></td>
            <td><%= result[i].user%></td>

            <td><%= result[i]._id%></td>
            <td><%= result[i].total%></td>
            <td><%= result[i].time%></td>
                                
            <td><%= result[i].status%></td>
         
              
                
                
                
               
                 
        <%}%>
    </tbody>
      
</table>
<script>
    $(document).ready(function() {
    $('#example').DataTable( {
        dom: 'Bfrtip',
        buttons: [
             'excel', 'pdf',
        ]
    } );
} );
</script>